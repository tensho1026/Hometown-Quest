generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String    @id 
  username  String?
  imageUrl  String?
  createdAt DateTime  @default(now())
  updateAt  DateTime  @default(now()) @updatedAt
}

model MstTodaysQuest {
  id Int @id @default(autoincrement())
  icon String
  title String @unique
  description String
  type String
  timer String
  level String
  point Int
  category String
}

model MstThisWeekQuest {
  id Int @id @default(autoincrement())
  icon String
  title String @unique
  description String
  type String
  timer String
  level String
  point Int
  category String
}